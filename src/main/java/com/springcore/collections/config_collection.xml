<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:p="http://www.springframework.org/schema/p"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
 	<!-- This is our beans -->
 	<bean name="emp1" class="com.springcore.collections.Emp">
 		<property name="name" value="Radhe"/>
 		<property name="phones">
 			<list>
 				<value>8750880677</value>
 				<value>7982257605</value>
 				<value>1234567</value>
 			</list>
 		</property>
 		<property name="addresses">
 			<set>
 				<value>Gorakhpur</value>
 				<value>Delhi</value>
 				<value>Noida</value>
 			</set>
 		</property>
 		<property name="courses">
 			<map>
 				<entry key="java" value="2 Months"/>
 				<entry key="python" value="1-2 Months"/>
 				<entry key="c" value="1 Months"/>
 			</map>
 		</property>
 		
 	</bean>
 
 </beans>